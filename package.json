{
  "name": "angular-vertxbus",
  "version": "3.0.0",
  "description": "AngularJS facade and service acting as a Vert.x SockJS client",
  "main": "dist/angular-vertxbus.js",
  "keywords": [
    "angular",
    "vertx",
    "facade",
    "websocket"
  ],
  "scripts": {
    "prepublish": "bower install",
    "test": "grunt install-test && grunt test",
    "test-scopes": "grunt install-test && for f in test_scopes/*; do TEST_SCOPE=\"`basename $f`\" grunt test; done",
    "clean-test-scopes": "for f in test_scopes/*; do (cd $f; rm -rf bower_components); done",
    "start-it-web-server": "babel-node test/e2e/server.js",
    "install-it-vertx-server": "cd test/e2e/vertx && ./install.sh",
    "start-it-vertx-server": "cd test/e2e/vertx/app && ../vert.x-2.1.1/bin/vertx run vertx2-config.js",
    "install-it-vertx3-server": "cd test/e2e/vertx && ./install-v3.sh",
    "start-it-vertx3-server": "cd test/e2e/vertx/app && ../vert.x-3.0.0/bin/vertx run vertx3-config.js"
  },
  "author": {
    "name": "Jan Philipp",
    "email": "knallisworld@googlemail.com"
  },
  "homepage": "http://github.com/knalli/angular-vertxbus",
  "repository": {
    "type": "git",
    "url": "git://github.com/knalli/angular-vertxbus"
  },
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.8.23",
    "babelify": "^6.2.0",
    "bower": "^1.5.2",
    "browserify": "^11.0.1",
    "browserify-ngannotate": "^1.0.1",
    "browserify-shim": "^3.8.10",
    "express": "~4.12.0",
    "grunt": "~0.4.5",
    "grunt-babel": "^5.0.1",
    "grunt-bower-install-simple": "^1.1.4",
    "grunt-browserify": "^4.0.0",
    "grunt-contrib-clean": "~0.6.0",
    "grunt-contrib-concat": "^0.5.1",
    "grunt-contrib-jshint": "~0.11.x",
    "grunt-contrib-uglify": "^0.9.2",
    "grunt-contrib-watch": "~0.6.1",
    "grunt-conventional-changelog": "^4.1.0",
    "grunt-extract-sourcemap": "^0.1.16",
    "grunt-karma": "^0.11.2",
    "grunt-ngdocs": "^0.2.9",
    "istanbul": "^0.3.18",
    "karma": "0.13.10",
    "karma-babel-preprocessor": "5.2.1",
    "karma-browserify": "^4.3.0",
    "karma-chrome-launcher": "^0.2.0",
    "karma-coverage": "^0.2.7",
    "karma-expect": "~1.1.0",
    "karma-firefox-launcher": "~0.1.3",
    "karma-mocha": "^0.2.0",
    "karma-phantomjs-launcher": "^0.2.1",
    "load-grunt-tasks": "~3.2.0",
    "lodash": "^3.10.1",
    "mocha": "^2.2.5"
  }
}
