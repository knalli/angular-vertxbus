{
  "name": "angular-vertxbus",
  "version": "6.0.0",
  "description": "AngularJS facade and service acting as a Vert.x SockJS client",
  "main": "dist/angular-vertxbus.js",
  "keywords": [
    "angular",
    "vertx",
    "facade",
    "websocket"
  ],
  "engines": {
    "node": ">=4.0"
  },
  "scripts": {
    "clean": "grunt clean",
    "clean-test-scopes": "for f in test_scopes/*; do (cd $f; rm -rf bower_components); done",
    "prepublish": "bower install",
    "lint": "eslint --ignore-pattern node_modules/**,bower_components/** --ext .js src/** *.js",
    "test-karma-headless": "grunt install-test && karma start --browsers='PhantomJS' karma.conf.js",
    "test-karma": "grunt install-test && karma start karma.conf.js",
    "test": "npm run -q lint && npm run -q test-karma",
    "test-scopes": "grunt install-test && for f in test_scopes/*; do TEST_SCOPE=\"`basename $f`\" grunt test; done",
    "start-it-web-server": "babel-node test/e2e/server.js",
    "install-it-vertx-server": "cd test/e2e/vertx && ./install-vertx.sh",
    "start-it-vertx-server": "cd test/e2e/vertx/app && ../runtime/bin/vertx run vertx3-config.js",
    "start-server": "npm-run-all --silent clean compile-dist install-it-vertx-server --parallel start-it-vertx-server start-it-web-server",
    "compile-dist": "webpack",
    "compile-distmin": "BUILD_MINIFIED=1 webpack",
    "compile": "grunt clean && npm run -q compile-dist && npm run -q compile-distmin && grunt build-post",
    "build": "npm run -q test && npm run -q compile",
    "release": "npm run -q clean && npm run -q build && grunt release"
  },
  "author": {
    "name": "Jan Philipp",
    "email": "knallisworld@googlemail.com"
  },
  "homepage": "http://github.com/knalli/angular-vertxbus",
  "repository": {
    "type": "git",
    "url": "git://github.com/knalli/angular-vertxbus"
  },
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.5.0",
    "babel-cli": "^6.5.0",
    "babel-core": "^6.0.2",
    "babel-eslint": "^6.0.1",
    "babel-istanbul-loader": "^0.1.0",
    "babel-loader": "^6.2.3",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-polyfill": "^6.0.14",
    "babel-preset-es2015": "^6.5.0",
    "bower": "1.7.7",
    "eslint": "^2.12.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-angular": "^1.0.0",
    "estraverse": "^4.1.0",
    "express": "^4.13.3",
    "grunt": "~0.4.5",
    "grunt-babel": "^6.0.0",
    "grunt-bower-install-simple": "~1.2.0",
    "grunt-cli": "^1.1.0",
    "grunt-contrib-clean": "~1.0.0",
    "grunt-contrib-concat": "^1.0.0",
    "grunt-conventional-changelog": "^6.1.0",
    "grunt-ngdocs": "^0.2.9",
    "imports-loader": "^0.6.5",
    "istanbul": "^0.4.0",
    "jasmine-core": "^2.3.4",
    "jasmine-webpack-plugin": "^0.1.1",
    "karma": "^0.13.11",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chrome-launcher": "^0.2.1",
    "karma-coverage": "^0.5.2",
    "karma-expect": "~1.1.0",
    "karma-firefox-launcher": "~0.1.3",
    "karma-jasmine": "^0.3.7",
    "karma-mocha": "^0.2.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "load-grunt-tasks": "^3.3.0",
    "lodash": "^4.0.1",
    "mocha": "^2.4.3",
    "ng-annotate-webpack-plugin": "^0.1.2",
    "npm-run-all": "^1.6.0",
    "phantomjs-prebuilt": "^2.1.3",
    "transform-loader": "^0.2.3",
    "webpack": "^1.12.13"
  }
}
